    },
    {
      "name": "finder",
      "prefix": "",
      "switcher_exclusive": true
    },
    {
      "name": "commands",
      "prefix": "",
      "switcher_exclusive": true
    },
    {
      "name": "websearch",
      "prefix": "?"
    },
    {
      "name": "switcher",
      "prefix": "/"
    }
  ]
